table.table.table-condensed
  tbody
    tr
      th Cmd Count
      td
        div(data-cdr={tlm:[{name:'/ULR/ULR_HkTlm_t/usCmdCnt'}]}) ---
    tr
      th Error Count
      td
        div(data-cdr={tlm:[{name:'/ULR/ULR_HkTlm_t/usCmdErrCnt'}]}) ---
    tr
      th Min Distance
      td
        div(data-cdr={tlm:[{name:'/ULR/ULR_HkTlm_t/MinDistance', format:'%.6f'}]}) ---
    tr
      th Max Distance
      td
        div(data-cdr={tlm:[{name:'/ULR/ULR_HkTlm_t/MaxDistance'}]}) ---
    tr
      th Current Distance
      td
        div(data-cdr={tlm:[{name:'/ULR/ULR_HkTlm_t/CurrentDistance', format:'%.6f'}]}) ---
    tr
      th Covariance
      td
        div(data-cdr={tlm:[{name:'/ULR/ULR_HkTlm_t/Covariance', format:'%.6f'}]}) ---
    tr
      th Sensor Type
      td
        div(data-cdr={tlm:[{name:'/ULR/ULR_HkTlm_t/Type', calibration:{ type:'enumeration',
