<varlistentry><term><anchor id="cfetimecmds_1_cfetimecmds000006"/>Global <ref refid="cfe__time__msg_8h_1a1380ec378fcade4628e7ccd85989b7f9" kindref="member">CFE_TIME_ADD_DELAY_CC</ref>  </term></varlistentry>
