<varlistentry><term><anchor id="cfssccfg_1_cfssccfg000002"/>Global <ref refid="sc__platform__cfg_8h_1ad698f831ae59d3e135dfeefcbe977b10" kindref="member">SC_NUMBER_OF_RTS</ref>  </term></varlistentry>
