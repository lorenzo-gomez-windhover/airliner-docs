<varlistentry><term><anchor id="cfsdscmds_1_cfsdscmds000014"/>Global <ref refid="ds__msgdefs_8h_1a0ac5bdc35d8ce4f9ccb0fa25a23d8712" kindref="member">DS_SET_DEST_COUNT_CC</ref>  </term></varlistentry>
<listitem><para>Set Sequence Count For Destination File Table Entry </para></listitem>
<varlistentry><term><anchor id="cfsdscmds_1_cfsdscmds000011"/>Global <ref refid="ds__msgdefs_8h_1a10c6b212013f9dd3c79e1d2a98615df5" kindref="member">DS_SET_DEST_EXT_CC</ref>  </term></varlistentry>
<listitem><para>Set Extension For Destination File Table Entry </para></listitem>
<varlistentry><term><anchor id="cfsdscmds_1_cfsdscmds000009"/>Global <ref refid="ds__msgdefs_8h_1a4bca0449b60b5bc6bf3c4fd7fe84e013" kindref="member">DS_SET_DEST_PATH_CC</ref>  </term></varlistentry>
