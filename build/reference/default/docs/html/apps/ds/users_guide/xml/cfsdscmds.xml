<varlistentry><term><anchor id="cfsdscmds_1_cfsdscmds000008"/>Global <ref refid="ds__msgdefs_8h_1a5a27ba0c1dca9d0a05ac81aa673ad422" kindref="member">DS_SET_DEST_STATE_CC</ref>  </term></varlistentry>
<listitem><para>Set Ena/Dis State For Destination File Table Entry </para></listitem>
<varlistentry><term><anchor id="cfsdscmds_1_cfsdscmds000007"/>Global <ref refid="ds__msgdefs_8h_1a88fa53fd482766adfee9e009444b101a" kindref="member">DS_SET_DEST_TYPE_CC</ref>  </term></varlistentry>
<listitem><para>Set Filename Type For Destination File Table Entry </para></listitem>
<varlistentry><term><anchor id="cfsdscmds_1_cfsdscmds000004"/>Global <ref refid="ds__msgdefs_8h_1a981b2fe81bccb90d8f09a8e5847db999" kindref="member">DS_SET_FILTER_FILE_CC</ref>  </term></varlistentry>
<listitem><para>Set Destination File For Packet Filter Table Entry </para></listitem>
<varlistentry><term><anchor id="cfsdscmds_1_cfsdscmds000006"/>Global <ref refid="ds__msgdefs_8h_1a2518be83bbbc5b5f439d0953bdf34ab2" kindref="member">DS_SET_FILTER_PARMS_CC</ref>  </term></varlistentry>
<listitem><para>Set Algorithm Parameters For Packet Filter Table Entry </para></listitem>
<varlistentry><term><anchor id="cfsdscmds_1_cfsdscmds000005"/>Global <ref refid="ds__msgdefs_8h_1a765facc4c0e6374257baddf51a0b04f7" kindref="member">DS_SET_FILTER_TYPE_CC</ref>  </term></varlistentry>
<listitem><para>Set Filter Type For Packet Filter Table Entry</para></listitem>
</variablelist>
