      <memberdef kind="define" id="md__platform__cfg_8h_1ade5e9fbb594c160bf10b0c5e220d25f0" prot="public" static="no">
        <name>MD_SIGNATURE_OPTION</name>
        <initializer>1</initializer>
        <briefdescription>
        </briefdescription>
        <detaileddescription>
<para><xrefsect id="cfsmdcfg_1_cfsmdcfg000005"><xreftitle>Purpose</xreftitle><xrefdescription><para>Option of whether a signature field will be reserved in dwell packets. 1 indicates &apos;yes&apos;, 0 indicates &apos;no&apos;.</para></xrefdescription></xrefsect></para><para><simplesect kind="par"><title>Limits</title><para>Value must be 0 or 1. </para></simplesect>
</para>        </detaileddescription>
        <inbodydescription>
        </inbodydescription>
        <location file="apps/md/fsw/platform_inc/md_platform_cfg.h" line="87" column="9" bodyfile="apps/md/fsw/platform_inc/md_platform_cfg.h" bodystart="87" bodyend="-1"/>
